{
    "manifest_version": 3,
    "name": "Ome.tv Automator",
    "version": "1.0.0",
    "action": { "default_popup": "index.html" },
    "permissions": ["storage"],
    "content_scripts": [
        {
            "matches": ["https://ome.tv/*"],
            "js": ["src/content/ometv.ts"]
        }
    ]
}
